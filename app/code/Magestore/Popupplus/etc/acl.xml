<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magestore_Popupplus::magestorepopupplus" title="Popupplus" sortOrder="10">
                    <resource id="Magestore_Popupplus::managepopupplus" title="Manage Popupplus" sortOrder="20" />
