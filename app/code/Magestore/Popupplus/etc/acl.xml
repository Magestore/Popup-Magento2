                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magestore_Popupplus::config_popupplus" title="Popupplus" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
